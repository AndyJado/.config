% FORMAT
%       setkey <function-name> <key-name>
%       unsetkey <key-name>
%       color <object-name> <fg> <bg>

% Color settings(Dark)
%color normal lightgray black
%color status black yellow
%color underline brightgreen black
%color overstrike brightblue black

% Color settings(Light)
color normal black reset
color status black white
color underline reset green
color overstrike reset green

% Keys
unsetkey "G"
unsetkey "g"
unsetkey "^g"
setkey down "e"
setkey down "E"
setkey down "j"
setkey down "^N"
setkey up "y"
setkey up "^Y"
setkey up "k"
setkey up "^P"
setkey page_down "f"
setkey page_down "^F"
setkey page_up "b"
setkey page_up "^B"
setkey other_window "z"
setkey other_window "w"
setkey search_backward "?"
setkey bob "g"
setkey eob "G"
setkey set_mark "m"
setkey goto_mark "'"
setkey find_file "E"
setkey edit "v"

